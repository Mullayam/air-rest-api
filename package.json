{
	"name": "air-rest-api-default",
	"version": "1.0.0",
	"main": "build/main.js",
	"scripts": {
		"build": "swc src -d build --source-maps --env.PURE=true",
		"build:stable": "bun build src/main.ts --outdir build --minify",
		"start": "bun  src/main.ts -o build",
		"install": "bun install",
		"watch": "bun run src/main.ts",
		"prestart": "node -r tsconfig-paths/register build/main.js",
		"dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
		"mig:create": "typeorm migration:create ./src/factory/migrations/%npm_config_name%",
		"mig:run": "npx typeorm-ts-node-esm migration:generate ./src/factory/migrations/%npm_config_name% -d ./src/app/config/Datasource.ts",
		"mig:revert": "typeorm migration:revert -- -d ./src/app/config/Datasource.ts",
		"entity:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm entity:create -n",
		"migrate:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n",
		"migrate:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
		"migrate:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert",
		"schema:drop": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm schema:drop",
		"lint": "biome check .",
		"lint:fix": "biome check --write . --ignore build ",
		"format": "biome format .",
		"format:fix": "biome check --fix --unsafe"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"biome check --write",
			"biome format --write"
		]
	},
	"author": "Mullayam",
	"license": "ISC",
	"dependencies": {
		"@biomejs/biome": "^1.9.4",
		"@biomejs/cli-darwin-x64": "^1.9.4",
		"@enjoys/exception": "^1.1.2",
		"@nestjs/cli": "^10.4.9",
		"@swc/cli": "^0.7.7",
		"@swc/core": "^1.11.31",
		"@swc/core-darwin-arm64": "^1.11.31",
		"@swc/core-darwin-x64": "^1.11.31",
		"@swc/core-linux-arm64-gnu": "^1.11.31",
		"@swc/helpers": "^0.5.17",
		"@types/glob": "^8.1.0",
		"@types/path-to-regexp": "^1.7.0",
		"bcrypt": "^5.1.1",
		"body-parser": "^1.20.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.2",
		"colorette": "^2.0.20",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"crypto-js": "^4.2.0",
		"debug": "^4.4.1",
		"dotenv": "^16.5.0",
		"express": "4.21.2",
		"express-fileupload": "^1.5.1",
		"express-rate-limit": "^7.5.0",
		"express-session": "^1.18.1",
		"express-validator": "^7.2.1",
		"glob": "^11.0.2",
		"googleapis": "^144.0.0",
		"helmet": "^8.1.0",
		"jsonwebtoken": "^9.0.2",
		"mime-types": "^2.1.35",
		"moment": "^2.30.1",
		"morgan": "^1.10.0",
		"nodemailer": "^6.10.1",
		"nodemailer-express-handlebars": "^7.0.0",
		"path-to-regexp": "^8.2.0",
		"pg": "^8.16.0",
		"rate-limiter-flexible": "^5.0.5",
		"redis": "^4.7.1",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.2",
		"socket.io": "^4.8.1",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"typeorm": "^0.3.24",
		"typescript": "^5.8.3",
		"validator": "^13.15.15",
		"winston": "^3.17.0"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/bcryptjs": "^2.4.6",
		"@types/body-parser": "^1.19.6",
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "^2.8.19",
		"@types/crypto-js": "^4.2.2",
		"@types/debug": "^4.1.12",
		"@types/express": "^5.0.3",
		"@types/express-fileupload": "^1.5.1",
		"@types/express-session": "^1.18.2",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/mime-types": "^2.1.4",
		"@types/morgan": "^1.9.10",
		"@types/node": "^22.15.30",
		"@types/node-cron": "^3.0.11",
		"@types/nodemailer": "^6.4.17",
		"@types/nodemailer-express-handlebars": "^4.0.5",
		"node-cron": "^3.0.3"
	}
}