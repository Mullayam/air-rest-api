{
  "name": "air-rest-api-default",
  "version": "1.0.0",
  "main": "build/main.js",
  "scripts": {
    "build": "nest build",
    "ready": "npm install",
    "start": "node ./build/main.js",
    "dev": "nest start --watch",
    "mig:create": "typeorm migration:create ./src/factory/migrations/%npm_config_name%",
    "mig:run": "npx typeorm-ts-node-esm migration:generate ./src/factory/migrations/%npm_config_name% -d ./src/app/config/Datasource.ts",
    "mig:revert": "typeorm migration:revert -- -d ./src/app/config/Datasource.ts",
    "entity:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm entity:create -n",
    "migrate:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migrate:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
    "migrate:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert",
    "schema:drop": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm schema:drop"
  },
  "author": "Mullayam",
  "license": "ISC",
  "dependencies": {
    "@enjoys/exception": "^1.1.2",
    "@nestjs/cli": "^10.4.5",
    "@swc/cli": "^0.4.1-nightly.20240914",
    "@swc/core": "^1.7.36",
    "@swc/helpers": "^0.5.13",
    "@types/glob": "^8.1.0",
    "@types/path-to-regexp": "^1.7.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "colorette": "^2.0.20",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "debug": "^4.3.7",
    "dotenv": "^16.3.1",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.0",
    "express-rate-limit": "^7.3.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.0.1",
    "glob": "^11.0.0",
    "googleapis": "^144.0.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.1",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.15",
    "nodemailer-express-handlebars": "^7.0.0",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.13.0",
    "rate-limiter-flexible": "^5.0.4",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.19",
    "typescript": "^5.6.3",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.2.2",
    "@types/debug": "^4.1.8",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/express-session": "^1.17.10",
