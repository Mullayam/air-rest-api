{
	"name": "air-rest-api-default",
	"version": "1.0.0",
	"main": "build/main.js",
	"scripts": {
		"build": "nest build",
		"ready": "npm install",
		"start": "node ./build/main.js",
		"dev": "nest start --watch",
		"mig:create": "typeorm migration:create ./src/factory/migrations/%npm_config_name%",
		"mig:run": "npx typeorm-ts-node-esm migration:generate ./src/factory/migrations/%npm_config_name% -d ./src/app/config/Datasource.ts",
		"mig:revert": "typeorm migration:revert -- -d ./src/app/config/Datasource.ts",
		"entity:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm entity:create -n",
		"migrate:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n",
		"migrate:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
		"migrate:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert",
		"schema:drop": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm schema:drop",
		"lint": "biome check .",
		"lint:fix": "biome check --write . --ignore build ",
		"format": "biome format .",
		"format:fix": "biome check --fix --unsafe"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": ["biome check --write", "biome format --write"]
	},
	"author": "Mullayam",
	"license": "ISC",
	"dependencies": {
		"@biomejs/biome": "^1.9.4",
		"@enjoys/exception": "^1.1.2",
		"@nestjs/cli": "^10.4.9",
		"@swc/cli": "^0.4.1-nightly.20240914",
		"@swc/core": "^1.10.9",
		"@swc/helpers": "^0.5.15",
		"@types/glob": "^8.1.0",
		"@types/path-to-regexp": "^1.7.0",
		"bcrypt": "^5.1.1",
		"body-parser": "^1.20.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"colorette": "^2.0.20",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"crypto-js": "^4.2.0",
		"debug": "^4.4.0",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"express-fileupload": "^1.5.1",
		"express-rate-limit": "^7.5.0",
		"express-session": "^1.18.1",
		"express-validator": "^7.2.1",
		"glob": "^11.0.1",
		"googleapis": "^144.0.0",
		"helmet": "^8.0.0",
		"jsonwebtoken": "^9.0.2",
		"mime-types": "^2.1.35",
		"moment": "^2.30.1",
		"morgan": "^1.10.0",
		"nodemailer": "^6.10.0",
		"nodemailer-express-handlebars": "^7.0.0",
		"path-to-regexp": "^8.2.0",
		"pg": "^8.13.1",
		"rate-limiter-flexible": "^5.0.5",
		"redis": "^4.7.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"socket.io": "^4.8.1",
		"tsconfig-paths": "^4.2.0",
		"typeorm": "^0.3.20",
		"typescript": "^5.7.3",
		"validator": "^13.12.0",
		"winston": "^3.17.0"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/bcryptjs": "^2.4.6",
		"@types/body-parser": "^1.19.5",
		"@types/cookie-parser": "^1.4.8",
		"@types/cors": "^2.8.17",
		"@types/crypto-js": "^4.2.2",
		"@types/debug": "^4.1.12",
		"@types/express": "^5.0.0",
		"@types/express-fileupload": "^1.5.1",
		"@types/express-session": "^1.18.1",
		"@types/jsonwebtoken": "^9.0.8",
		"@types/mime-types": "^2.1.4",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.10.10",
		"@types/node-cron": "^3.0.11",
		"@types/nodemailer": "^6.4.17",
		"@types/nodemailer-express-handlebars": "^4.0.5",
		"node-cron": "^3.0.3",
		"ts-node": "^10.9.2"
	}
}
